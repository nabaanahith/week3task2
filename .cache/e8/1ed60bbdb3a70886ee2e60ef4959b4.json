en((data)=>{\n      this.setState({\n        news: data.articles\n      })\n    })\n  }\n\n\n  render() {\n    return (\n      <React.Fragment>\n        <Navigation>\n          <img width=\"150px;\" src={require('./assets/logo.svg')}/>\n          <SearchBox placeholder=\"search term\"/>\n        </Navigation>\n        <NewsContainer>\n          {\n            this.state.news.map((item, i)=>{\n              return (\n              <NewsItem>\n                  <img width=\"124px;\" height=\"124px\" src={item.urlToImage} />\n                <NewsText>\n                  <h1>{item.title}</h1>\n                  <p>{item.description}</p>\n                  <DateTime>{item.publishedAt}</DateTime>\n                </NewsText>\n              </NewsItem>\n              )\n            })\n          }\n        </NewsContainer>\n      </React.Fragment>\n    )\n  }\n}\n\nfunction App() {\n  return <div>\n    <News/>\n  </div>\n}\n\nReactDOM.render(<App/>, document.getElementById('root'))\n\n"},"lineCount":null}},"hash":"08d2878ff0f6ed260646925e8f6e1169","cacheData":{"env":{}}}