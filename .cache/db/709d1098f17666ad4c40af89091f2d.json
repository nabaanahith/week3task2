enerated":{"line":187,"column":0},"source":"app.js","original":{"line":133,"column":0}},{"name":"render","generated":{"line":187,"column":18},"source":"app.js","original":{"line":133,"column":9}},{"generated":{"line":187,"column":24},"source":"app.js","original":{"line":133,"column":0}},{"generated":{"line":187,"column":25},"source":"app.js","original":{"line":133,"column":16}},{"generated":{"line":187,"column":54},"source":"app.js","original":{"line":133,"column":17}},{"generated":{"line":187,"column":57},"source":"app.js","original":{"line":133,"column":16}},{"generated":{"line":187,"column":64},"source":"app.js","original":{"line":133,"column":0}},{"name":"document","generated":{"line":187,"column":66},"source":"app.js","original":{"line":133,"column":24}},{"generated":{"line":187,"column":74},"source":"app.js","original":{"line":133,"column":32}},{"name":"getElementById","generated":{"line":187,"column":75},"source":"app.js","original":{"line":133,"column":33}},{"generated":{"line":187,"column":89},"source":"app.js","original":{"line":133,"column":24}},{"generated":{"line":187,"column":90},"source":"app.js","original":{"line":133,"column":48}},{"generated":{"line":187,"column":96},"source":"app.js","original":{"line":133,"column":24}},{"generated":{"line":187,"column":97},"source":"app.js","original":{"line":133,"column":0}}],"sources":{"app.js":"\n// import necesary libs.\nimport React, { Component } from 'react'\nimport ReactDOM from 'react-dom'\nimport styled from 'styled-components'\n\n\nlet SearchBox = styled.input `\n  border-radius: 20px;\n  background-color: #000;\n  color: #fff;\n  font-size: 1.2rem;\n  border: 0px;\n  height: 40px;\n  outline: none;\n  padding: 0 10px;\n`\nlet Navigation = styled.header `\n  display: flex;\n  padding: 0px 10%;\n  align-items: center;\n  justify-content: space-between;\n  box-shadow: 0px 2px 25px rgba(0,0,0,0.16);\n  height: 100px;\n`\n\nlet NewsContainer = styled.main`\n  background-color: red;\n  padding: 20px 10%;\n\n`\n\nlet NewsItem = styled.div`\n  background-color: #fff;\n  border: 2px solid #E5E9F2;\n  min-height: 150px;\n  margin: 20px 0px;\n  border-radius: 4px;\n  display: flex;\n  padding: 10px;\n`\n\nlet NewsText = styled.div`\n  padding-left: 14px;\n  position: relative;\n`\n\nlet DateTime = styled.time`\n  position: absolute;\n  bottom: 0px;\n  color: #399DF2;\n  font-family: sans-serif;\n`\n\nclass News extends Component{\n  \n  constructor(){\n    super()\n  \n    this.state = {\n      news: [],\n      searchValue: ''\n    }\n\n    this.getNews()\n\n  }\n\n  getNews(searchTerm = 'Iraq') {\n    fetch(`https://newsapi.org/v2/everything?q=${searchTerm}&apiKey=978d6c3818ff431b8c210ae86550fb1f`)\n    .then((response)=>{\n      return response.json()\n    })\n    .then((data)=>{\n      this.setState({\n        news: data.articles\n      })\n    })\n  }\n\n  onInputChange(event){\n    this.setState({\n      searchValue: event.target.value\n    })\n  } \n\n  onKeyUp(event){\n    if(event.key == 'Enter'){\n      this.getNews(this.state.searchValue)\n      this.setState({\n        searchValue: ''\n      })\n    }\n  }\n\n  render() {\n    return (\n      <React.Fragment>\n        <Navigation>\n          <img width=\"150px;\" src={require('./assets/logo.svg')}/>\n          <SearchBox \n          onChange={this.onInputChange.bind(this)} \n          onKeyUp={this.onKeyUp.bind(this)}\n          value={this.state.searchValue} placeholder=\"search term\"/>\n        </Navigation>\n        <NewsContainer>\n          {\n            this.state.news.map((item, i)=>{\n              return (\n              <NewsItem key={i}>\n                  <img width=\"124px;\" height=\"124px\" src={item.urlToImage} />\n                <NewsText>\n                  <h1>{item.title}</h1>\n                  <p>{item.description}</p>\n                  <DateTime>{item.publishedAt}</DateTime>\n                </NewsText>\n              </NewsItem>\n              )\n            })\n          }\n        </NewsContainer>\n      </React.Fragment>\n    )\n  }\n}\n\nfunction App() {\n  return <div>\n    <News/>\n  </div>\n}\n\nReactDOM.render(<App/>, document.getElementById('root'))\n\n"},"lineCount":null}},"hash":"ffddb667e2d4bc22f5c0b7b6021657a3","cacheData":{"env":{}}}